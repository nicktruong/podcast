  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage --watchAll=false",
    "test:interactive": "craco test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint:check": "npx eslint src/**/*.{ts,tsx}",
    "lint:write": "npx eslint src/**/*.{ts,tsx} --fix",
    "prettier:check": "npx prettier . --check",
    "prettier:write": "npx prettier . --write",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nick-q1 --project podcast-website ./build && sentry-cli sourcemaps upload --org nick-q1 --project podcast-website ./build",
    "storybook": "yarn tailwind && storybook dev -p 6006",
    "build-storybook": "yarn tailwind && storybook build",
    "tailwind": "npx tailwindcss -c ./tailwind.config.js -i ./src/index.css -o .tailwind/index.css",
    "seed": "ts-node -r tsconfig-paths/register src/seeds"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^18.4.3",
